<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#75358B"
    android:textAlignment="center"
    tools:context=".Reserva">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="541dp"
        android:layout_height="543dp"
        android:layout_marginTop="84dp"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="3dp">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="360dp"
            android:text="Servicios"
            android:textSize="40sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/editTextTextPersonName6"
            android:layout_width="362dp"
            android:layout_height="69dp"
            android:layout_marginTop="56dp"
            android:background="#FFFBFE"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Servicio"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.256"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

        <ImageButton
            android:id="@+id/imageButton11"
            android:layout_width="52dp"
            android:layout_height="54dp"
            android:layout_marginStart="244dp"
            android:layout_marginTop="4dp"
            android:background="#FFFBFE"
            app:layout_constraintStart_toStartOf="@+id/editTextTextPersonName6"
            app:layout_constraintTop_toTopOf="@+id/editTextTextPersonName6"
            app:srcCompat="@drawable/ic_baseline_edit_off_24" />

        <ImageButton
            android:id="@+id/imageButton12"
            android:layout_width="52dp"
            android:layout_height="54dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="4dp"
            android:background="#FFFBFE"
            app:layout_constraintStart_toEndOf="@+id/imageButton11"
            app:layout_constraintTop_toTopOf="@+id/editTextTextPersonName6"
            app:srcCompat="@drawable/ic_baseline_close_24" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>