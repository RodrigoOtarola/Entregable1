<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#75358B"
    tools:context=".Menu_Realizar_Reserva">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="411dp"
        android:layout_height="353dp"
        android:layout_marginTop="148dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.238"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <EditText
            android:id="@+id/editTextTextPersonName8"
            android:layout_width="353dp"
            android:layout_height="63dp"
            android:layout_marginStart="52dp"
            android:layout_marginTop="48dp"
            android:background="#A662BB"
            android:drawableEnd="@drawable/ic_baseline_arrow_drop_down_24"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Local"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <EditText
            android:id="@+id/editTextTextPersonName12"
            android:layout_width="353dp"
            android:layout_height="63dp"
            android:layout_marginStart="52dp"
            android:layout_marginTop="28dp"
            android:background="#A662BB"
            android:drawableEnd="@drawable/ic_baseline_date_range_24"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Fecha"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName8" />

        <EditText
            android:id="@+id/editTextTextPersonName13"
            android:layout_width="353dp"
            android:layout_height="63dp"
            android:layout_marginStart="52dp"
            android:layout_marginTop="28dp"
            android:background="#A662BB"
            android:drawableEnd="@drawable/ic_baseline_arrow_drop_down_24"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Servicio"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName12" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <com.google.android.material.bottomappbar.BottomAppBar
        android:id="@+id/bottomAppBar"
        android:layout_width="479dp"
        android:layout_height="53dp"
        android:layout_gravity="bottom"
        android:background="#A662BB"
        app:backgroundTint="#A662BB"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.998"
        app:navigationIcon="@drawable/ic_baseline_menu_24"
        tools:layout_editor_absoluteX="1dp"
        tools:ignore="BottomAppBar,MissingConstraints" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="478dp"
        android:layout_height="53dp"
        android:layout_marginTop="1020dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3"
        tools:layout_editor_absoluteX="1dp">

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="61dp"
            android:layout_height="50dp"
            android:background="#A662BB"
            app:srcCompat="@drawable/ic_baseline_person_24"
            tools:layout_editor_absoluteX="417dp"
            tools:layout_editor_absoluteY="1dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="410dp"
        android:layout_height="93dp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3"
        tools:layout_editor_absoluteX="0dp">

        <ImageButton
            android:id="@+id/imageButton8"
            android:layout_width="78dp"
            android:layout_height="66dp"
            android:layout_marginStart="52dp"
            android:layout_marginTop="16dp"
            android:background="#75358B"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_baseline_replay_24" />

        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="78dp"
            android:layout_height="66dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="44dp"
            android:background="#75358B"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_baseline_check_24" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

